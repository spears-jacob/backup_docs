        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND message__name = 'Reset-Equipment-Voice-Success' , visit__account__enc_account_number, Null))) AS voice_equipment_reset_flow_successes_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('footer-manageaccount-accountsummary') , visit__account__enc_account_number, Null))) AS footer_manageaccount_account_summary_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('footer-getsupport-accountsupport') , visit__account__enc_account_number, Null))) AS footer_getsupport_acct_support_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('footer-manageaccount-billing') , visit__account__enc_account_number, Null))) AS footer_manageaccount_billing_households,
