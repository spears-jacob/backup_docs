set hive.vectorized.execution.enabled = false;

USE prod;

DROP TABLE IF EXISTS ${env:TMP_db}.asp_counts_daily_counts_households_columns;
CREATE TEMPORARY TABLE ${env:TMP_db}.asp_counts_daily_counts_households_columns AS
