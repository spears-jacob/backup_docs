        'L-CHTR' as company,
        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND state__view__current_page__name = 'home-authenticated' , visit__account__enc_account_number, Null))) AS auth_homepage_page_views_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS set_up_auto_payment_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS set_up_auto_payment_successes_households,
        SIZE(COLLECT_SET(IF( state__content__details__episode_title RLIKE '.*bp:autopay.*', visit__account__enc_account_number, Null))) AS autopay_is_enabled_households,
        SIZE(COLLECT_SET(IF( state__content__details__episode_title RLIKE '.*bp:no-autopay.*', visit__account__enc_account_number, Null))) AS autopay_is_not_enabled_households,
