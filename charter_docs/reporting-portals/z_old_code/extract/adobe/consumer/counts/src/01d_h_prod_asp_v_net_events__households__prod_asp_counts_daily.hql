        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND message__name = 'reset-equipment-tv-failure' , visit__account__enc_account_number, Null))) AS tv_equipment_reset_flow_failures_households,
        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND message__name = 'Reset-Equipment-Voice-Failure' , visit__account__enc_account_number, Null))) AS voice_equipment_reset_flow_failures_households,
