        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('hamburger-signout') , visit__account__enc_account_number, Null))) AS hamburger_signout_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('hamburger-voice') , visit__account__enc_account_number, Null))) AS hamburger_voice_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('hamburger-watchtv') , visit__account__enc_account_number, Null))) AS hamburger_watch_tv_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS iva_opens_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('localnav-accountsummary') , visit__account__enc_account_number, Null))) AS localnav_acct_summary_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('localnav-billing') , visit__account__enc_account_number, Null))) AS localnav_billing_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('localnav-internet') , visit__account__enc_account_number, Null))) AS localnav_internet_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('localnav-settings') , visit__account__enc_account_number, Null))) AS localnav_settings_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('localnav-tv') , visit__account__enc_account_number, Null))) AS localnav_tv_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('localnav-voice') , visit__account__enc_account_number, Null))) AS localnav_voice_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS login_attempts_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS manual_reset_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS manual_reset_starts_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS manual_reset_success_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS manual_troubleshoot_page_views_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS internet_modem_reset_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS voice_modem_reset_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS internet_modem_reset_successes_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS voice_modem_reset_successes_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS internet_modem_router_reset_starts_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS voice_modem_router_reset_starts_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS one_time_payment_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS one_time_payment_starts_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS one_time_payment_successes_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS otp_with_autopay_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS otp_with_autopay_successes_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS os_all_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS os_android_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS os_ios_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS os_ios_or_android_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS os_not_ios_or_android_households,
        SIZE(COLLECT_SET(IF( state__content__details__episode_title RLIKE '.*pl:pl-yes.*', visit__account__enc_account_number, Null))) AS paperless_billing_is_enabled_households,
        SIZE(COLLECT_SET(IF( state__content__details__episode_title RLIKE '.*pl:pl-no.*', visit__account__enc_account_number, Null))) AS paperless_billing_is_not_enabled_households,
        SIZE(COLLECT_SET(IF( (( message__category = 'Page View'  AND state__view__current_page__name IN('Reset-1.bam','Reset-1.btm')) OR (message__category = 'Page View' AND state__view__current_page__name = 'Reset-1.nbtm')), visit__account__enc_account_number, Null))) AS password_reset_attempts_households,
        SIZE(COLLECT_SET(IF( ((message__category = 'Page View' AND state__view__current_page__name IN ('Reset-final.bam','Reset-final.btm')) Or (message__category = 'Page View' AND state__view__current_page__name = 'Reset-final.nbtm')), visit__account__enc_account_number, Null))) AS password_reset_successes_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS refresh_set_top_box_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS refresh_set_top_box_requests_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS rescheduled_service_appointments_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS internet_router_reset_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS voice_router_reset_failures_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS internet_router_reset_successes_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS voice_router_reset_successes_households,
        SIZE(COLLECT_SET(IF( state__view__current_page__search_text IS NOT NULL, visit__account__enc_account_number, Null))) AS search_action_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS search_results_clicked_households,
        SIZE(COLLECT_SET(IF( TRUE, visit__account__enc_account_number, Null))) AS site_unique_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS site_unique_auth_households,
