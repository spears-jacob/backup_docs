        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) = LOWER('footer-contact-websiteFeedback') , visit__account__enc_account_number, Null))) AS footer_contactus_give_website_feedback_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('footer-legal-gotoassist') , visit__account__enc_account_number, Null))) AS footer_legal_go_to_assist_households,
