        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND state__view__current_page__name = 'home-unauth' , visit__account__enc_account_number, Null))) AS unauth_homepage_page_views_households,
        SIZE(COLLECT_SET(IF( ((message__category = 'Page View' AND state__view__current_page__name IN('Recover-1.btm','Recover-1.bam')) Or (message__category = 'Page View' AND state__view__current_page__name = 'Recover-1.nbtm' AND state__view__current_page__name NOT IN  ('Recover-noID.nbtm'))), visit__account__enc_account_number, Null))) AS username_recovery_attempts_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('utilitynav-email') , visit__account__enc_account_number, Null))) AS utilitynav_email_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('utilitynav-support') , visit__account__enc_account_number, Null))) AS utilitynav_support_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('utilitynav-voice') , visit__account__enc_account_number, Null))) AS utilitynav_voice_households,
        SIZE(COLLECT_SET(IF( ((message__category = 'Custom Link') AND message__name IN('View Current Bill','Statement','View Statement','pay-bill.billing-statement-download') OR (message__category = 'Custom Link' AND message__name IN('pay-bill.view-statements') AND visit__settings['post_prop26'] IS NOT NULL)), visit__account__enc_account_number, Null))) AS view_online_statement_households,
        SIZE(COLLECT_SET(IF( FALSE, visit__account__enc_account_number, Null))) AS webmail_views_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('internet') , visit__account__enc_account_number, Null))) AS yourservicesequip_internet_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('mobile') , visit__account__enc_account_number, Null))) AS yourservicesequip_mobile_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('tv') , visit__account__enc_account_number, Null))) AS yourservicesequip_tv_households,
        SIZE(COLLECT_SET(IF( message__category = 'Custom Link'  AND LOWER(message__name) IN ('voice') , visit__account__enc_account_number, Null))) AS yourservicesequip_voice_households,
        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND (LOWER(message__name) = LOWER('Wait-15-Error-Modal-TV') OR LOWER(message__name) = LOWER('Wait-15-Error-Modal-Internet') OR LOWER(message__name) = LOWER('Wait-15-Error-Modal-Voice')) , visit__account__enc_account_number, Null))) AS equipment_resets_too_many_resets_modal_all_households,
        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND LOWER(message__name) = LOWER('Wait-15-Error-Modal-Internet') , visit__account__enc_account_number, Null))) AS equipment_resets_too_many_resets_modal_internet_households,
        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND LOWER(message__name) = LOWER('Wait-15-Error-Modal-TV') , visit__account__enc_account_number, Null))) AS equipment_resets_too_many_resets_modal_tv_households,
        SIZE(COLLECT_SET(IF( message__category = 'Page View'  AND LOWER(message__name) = LOWER('Wait-15-Error-Modal-Voice') , visit__account__enc_account_number, Null))) AS equipment_resets_too_many_resets_modal_voice_households,
        'asp' AS platform,
        'resi' AS domain,
        
        epoch_converter(cast(message__timestamp * 1000 as bigint),'America/Denver') as date_denver
    FROM asp_v_net_events
         
    WHERE (partition_date >= '${env:START_DATE}'
       AND partition_date <  '${env:END_DATE}')
    GROUP BY
        epoch_converter(cast(message__timestamp * 1000 as bigint),'America/Denver'),
        
        'L-CHTR'
    ;
INSERT OVERWRITE TABLE prod.asp_counts_daily
PARTITION(unit,platform,domain,company,date_denver,source_table)

    SELECT  value,
            metric,
            
            'households',
            'asp',
            'resi',
            company,
            date_denver,
            'asp_v_net_events'
    FROM (SELECT  company,
                  date_denver,
                  
                  MAP(

                      'auth_homepage_page_views|Authenticated Homepage Page Views|Spectrum.net_adobe||', auth_homepage_page_views_households,
                      'set_up_auto_payment_failures|Auto Pay Enrollment Failures|Spectrum.net_adobe||', set_up_auto_payment_failures_households,
                      'set_up_auto_payment_successes|Auto Pay Enrollment Successes|Spectrum.net_adobe||', set_up_auto_payment_successes_households,
                      'autopay_is_enabled|Autopay Is Enabled|Spectrum.net_adobe||', autopay_is_enabled_households,
                      'autopay_is_not_enabled|Autopay Is Not Enabled|Spectrum.net_adobe||', autopay_is_not_enabled_households,
                      'bill_status_past_due_no|Bill Status Past Due No|Spectrum.net_adobe||', bill_status_past_due_no_households,
                      'bill_status_past_due_yes|Bill Status Past Due Yes|Spectrum.net_adobe||', bill_status_past_due_yes_households,
                      'cancelled_service_appointments|Cancelled Service Appointments|Spectrum.net_adobe||', cancelled_service_appointments_households,
                      'contact_us_page_views|Contact Us Page Views|Spectrum.net_adobe||', contact_us_page_views_households,
                      'create_username_referred_from_msa|Create Username Referral From MySpectrum App|Spectrum.net_adobe||', create_username_referred_from_msa_households,
                      'equipment_page_views|Equipment Flow Page Views|Spectrum.net_adobe||', equipment_page_views_households,
                      'internet_equipment_reset_flow_failures|Equipment Reset Flow Failures Internet|Spectrum.net_adobe||', internet_equipment_reset_flow_failures_households,
                      'tv_equipment_reset_flow_failures|Equipment Reset Flow Failures TV|Spectrum.net_adobe||', tv_equipment_reset_flow_failures_households,
                      'voice_equipment_reset_flow_failures|Equipment Reset Flow Failures Voice|Spectrum.net_adobe||', voice_equipment_reset_flow_failures_households,
                      'internet_equipment_reset_flow_starts|Equipment Reset Flow Starts Internet|Spectrum.net_adobe||', internet_equipment_reset_flow_starts_households,
                      'tv_equipment_reset_flow_starts|Equipment Reset Flow Starts TV|Spectrum.net_adobe||', tv_equipment_reset_flow_starts_households,
                      'voice_equipment_reset_flow_starts|Equipment Reset Flow Starts Voice|Spectrum.net_adobe||', voice_equipment_reset_flow_starts_households,
